using System;
					
public class Program
{
	public static void Main()
	{
		int[] arr = {5,7,8,1,2,9,2,3,4};
		int biggestNumber = arr[0];
		for(int i=1;i< arr.Length; i++)
		{
			if(arr[i] > biggestNumber)
			{
				biggestNumber = arr[i];
			}
		}
		Console.WriteLine(biggestNumber);
	}
}

Input
[5,7,8,1,2,9,2,3,4]
Output
9
